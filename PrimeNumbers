def nombreprem(a):
    nbdiviseurs = 0
    for i in range(2,a-1):
        if nbdiviseurs >= 1:
            break 
        elif a%i == 0: 
            nbdiviseurs = nbdiviseurs+1
    if nbdiviseurs >= 1: 
        return 'Pas un nombre premier...'
    else:
        return 'Nombre premier!'

var = int(input('Quel nombre tester? '))
print(nombreprem(var))
